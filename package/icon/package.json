{
  "name": "@xunserver/icon",
  "version": "0.0.8",
  "description": "",
  "main": "./dist/lib/index.js",
  "module": "./dist/es/index.js",
  "exports": {
    ".": {
      "import": "./dist/es/index.js",
      "require": "./dist/lib/index.js"
    }
  },
  "scripts": {
    "g": "npm run prebuild",
    "prebuild": "npm run clean && pnpm tsc --outdir .gulp && cross-env NODE_ENV=production pnpm gulp -f .gulp/gulpfile.js --cwd ./ -LLLL",
    "clean": "rimraf '.gulp' './src' './dist'",
    "build:es": "cross-env NODE_ENV=production tsc --project tsconfig.build.json --module esnext --outDir dist/es",
    "build:lib": "cross-env NODE_ENV=production tsc --project tsconfig.build.json --module CommonJS --outDir dist/lib",
    "build": "npm run g && run-p build:*"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@reactivex/rxjs": "^6.6.7",
    "@rgrove/parse-xml": "^3.0.0",
    "@types/ejs": "^3.1.1",
    "@types/gulp": "^4.0.10",
    "@types/gulp-rename": "^2.0.1",
    "@types/lodash": "^4.14.185",
    "@types/svgo": "^2.6.4",
    "@types/through2": "^2.0.36",
    "@types/vinyl": "^2.0.6",
    "@xunserver/shell": "^0.0.7",
    "@xunserver/util": "^0.0.3",
    "ejs": "^3.1.8",
    "gulp": "^4.0.2",
    "gulp-rename": "^2.0.0",
    "lodash": "^4.17.21",
    "svgo": "^2.8.0",
    "through2": "^3.0.2"
  },
  "files": [
    "dist",
    "README.md"
  ]
}
